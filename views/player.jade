//- From node:
//- pageData.title: Sample Title
//- pageData.mp4: /path/to/mp4/file
//- pageData.ogg: /path/to/ogg/file
//- pageData.webm: /path/to/webm/file
//-
extends ./layout.jade

block content
    body
        video(width="800",controls)
            source(id='mp4',src='/streaming?get=' + pageData.mp4,type="video/mp4")
            source(id='ogg',src='/streaming?get=' + pageData.ogg,type="video/ogg")
            source(id='webm',src='/streaming?get=' + pageData.webm,type="video/webm")
            Your browser does not support HTML5 video.
        br